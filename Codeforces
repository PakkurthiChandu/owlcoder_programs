#include<bits/stdc++.h>
#include<cmath>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a,b,c,d,e,f,g,h;
        cin>>a>>b;
        cin>>c>>d;
        cin>>e>>f;
        cin>>g>>h;
        if(a==c)
        {
            cout<<(d-b)*(d-b)<<endl;
        }
        else if(a==e)
        {
            cout<<(f-b)*(f-b)<<endl;
        }
        else
        {
            cout<<(h-b)*(h-b)<<endl;
        }
    }
}
